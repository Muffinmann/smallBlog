{
    "env": {
        "node": true,
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "space-before-blocks": ["error"],
        "keyword-spacing": ["error"],
        "block-spacing": ["error"],
        "arrow-spacing": ["error"],
        "brace-style": "error",
        "no-const-assign":"error",
        "indent": ["error", 2]
    }
}
